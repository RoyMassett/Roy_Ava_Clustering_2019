    "\n",
    "# you can read directly from an URL as well\n",
    "irisdf = pd.read_csv(\"https://courses.cs.ut.ee/MTAT.03.227/2018_spring/uploads/Main/iris.csv\", header = None)\n",
    "\n",
    "# we can assign (new) column names, it goes from left to right\n",
    "irisdf.columns = [\"sepal_length\", \"sepal_width\", \"petal_length\", \"petal_width\", \"species\"]\n",
    "\n",
    "# see the data, this is it actually depicted\n",
    "irisdf.head()\n",
    "\n",
    "# data size, this gives the dimensions in a sense (down, across)\n",
    "irisdf.shape\n",
    "\n",
    "# ask for specific statistic of a column, gives state on 'sepal_length'\n",
    "irisdf.sepal_length.mean()\n",
    "\n",
    "# or of all columns, a little summary of what is happening\n",
    "irisdf.mean()"
